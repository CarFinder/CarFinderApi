{
  "name": "car-finder",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev-server": "nodemon -w src -x ts-node  src/index.ts",
    "test:watch": "mocha --opts mocha.opts --watch",
    "start": "ts-node src/index.ts",
    "test": "mocha --timeout 15000 -r ts-node/register spec/**/*.ts",
    "debug": "tsc --watch",
    "test:coverage": "nyc  mocha --timeout 15000 -r ts-node/register spec/**/*.ts && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc  mocha --timeout 15000 -r ts-node/register spec/**/*.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".js",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "license": "MIT",
  "devDependencies": {
